<?xml version="1.0" encoding="utf-8"?>
<!--
  SWBF2-AdminHelper 3.0
  JW "LeKeks" 03/2015
  Command Configuration
  Note: if you really mess up one of the configuration files just delete it and run the application,
		it'll create a new, clean configuration file.
-->
<CmdPutGroup xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Permission>putgroup</Permission>
  <IsPublic>false</IsPublic>
  <CommandAlias>putgroup</CommandAlias>
  <OnPut>%u was put into user group %g.</OnPut>
  <OnSyntaxError>Syntax: !putgroup &lt;user&gt; &lt;group&gt;</OnSyntaxError>
  <OnNoPlayerMatch>No player matching %e could be found!</OnNoPlayerMatch>
  <OnNoGroupMatch>No group matching %e could be found!</OnNoGroupMatch>
</CmdPutGroup>